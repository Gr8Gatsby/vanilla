{
  "name": "react-datatable",
  "private": true,
  "version": "0.0.1",
  "description": "A datatable component in React",
  "license": "MIT",
  "dependencies": {
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "build": "react-scripts build && yarn build:sfdx",
    "build:sfdx": "cp build/static/js/main*.js ../../force-app/main/default/staticresources/react/main.js && cp build/static/css/main*.css ../../force-app/main/default/staticresources/react/main.css",
    "clean": "rm -rf build/static",
    "lint": "yarn build",
    "test": "react-scripts test --env=jsdom",
    "start": "react-scripts start",
    "eject": "react-scripts eject"
  }
}
